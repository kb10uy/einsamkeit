  "homepage": "https://einsamkeit.kb10uy.org",
  "license": "ISC",
  "main": "index.js",
  "keywords": [
    "ActivityPub"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kb10uy/einsamkeit.git"
  },
  "bugs": {
    "url": "https://github.com/kb10uy/einsamkeit/issues"
  },
  "scripts": {
    "compile": "tsc",
    "precommit": "lint-staged",
    "lint": "tslint -c ./tslint.json -p ./ --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "db:migrate:make": "knex migrate:make",
    "db:migrate": "knex migrate:latest",
    "server:serve": "ts-node -P ./tsconfig.json ./src/index.ts",
    "server:production": "cross-env NODE_ENV=production ts-node -P ./tsconfig.json ./src/index.ts"
