    "@types/koa": "^2.0.46",
    "@types/koa-bodyparser": "^5.0.1",
    "@types/koa-router": "^7.0.31",
    "@types/log4js": "^2.3.5",
    "@types/node": "^10.9.2",
    "@types/uuid": "^3.4.3",
    "bull": "^3.4.7",
    "config": "^2.0.1",
    "cross-env": "^5.2.0",
    "knex": "^0.15.2",
    "koa": "^2.5.2",
    "koa-bodyparser": "^4.2.1",
    "koa-router": "^7.4.0",
    "log4js": "^3.0.4",
